#include "Keyboard.h"

void typeKey(uint8_t key)
{
  Keyboard.press(key);
  delay(50);
  Keyboard.release(key);
}

/* Init function */
void setup()
{
  // Begining the Keyboard stream
  Keyboard.begin();

  // Wait 500ms
  delay(500);

  Keyboard.press(KEY_LEFT_CTRL);
  Keyboard.press(KEY_ESC);
  Keyboard.releaseAll();

  delay(200);
  Keyboard.print("notepad");

  typeKey(KEY_RETURN);

  delay(200);

  typeKey(KEY_RETURN);
  typeKey(KEY_RETURN);
  typeKey(KEY_RETURN);
  typeKey(KEY_RETURN);
 Keyboard.print ("WB HACKING!!!!!");

  typeKey(KEY_RETURN);

  // Ending stream
  Keyboard.end();
}

void loop() {}
